{
  "name": "Empire",
  "description": "A window on the Empire State Building, wherever you are.",
  "version": "1.0.2",
  "manifest_version": 2,
  "homepage_url": "http://tinktank.it/empire/", 
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "chrome_url_overrides" : {
    "newtab": "index.htm"
  },
  "permissions": [
    "tabs",
    "http://tinktank.it/empire/*"
  ],
  "browser_action": {}
}